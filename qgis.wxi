    <?error QGIS package $(var.AppPackage) not supported yet.. ?>
  <?endif ?>

  <?define ShortcutName=$(var.AppName)?>
  <?define AppIcon="$(var.QGISPATH)\apps\$(var.AppPackage)\icons\qgis.ico" ?>
</Include>
