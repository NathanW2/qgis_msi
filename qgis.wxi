<?xml version="1.0" encoding="utf-8"?> 
<Include>
  <?if $(var.AppPackage) = qgis-ltr ?>
    <?define AppVersionMajor="2.14"?>
    <?define AppVersion="2.14.8"?>
    <?define AppName="QGIS $(var.AppVersion)"?>
    <?define AppRoot="QGIS $(var.AppVersionMajor)"?>
